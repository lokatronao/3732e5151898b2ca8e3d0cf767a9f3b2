    },
    "homepage": "https://github.com/lokatronao/3732e5151898b2ca8e3d0cf767a9f3b2#readme",
    "devDependencies": {
        "@types/express": "^4.16.1",
        "@types/mocha": "^5.2.6",
        "@types/mongoose": "^5.3.24",
        "chai": "^4.2.0",
        "chai-http": "^4.2.1",
        "coveralls": "^3.0.3",
        "istanbul": "^0.4.5",
        "mocha": "^6.0.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^13.3.0",
        "ts-node": "^8.0.3",
        "typescript": "^3.4.2"
    },
    "nyc": {
        "include": [
            "**/*.ts"
        ],
        "exclude": [
            "**/*.test.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "dependencies": {
        "bcrypt": "^3.0.5",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "express-fileupload": "^1.1.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.4.22"
